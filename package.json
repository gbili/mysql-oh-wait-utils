    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "7.0.1",
    "@types/mysql": "^2.15.21",
    "@types/node": "^13.7.1",
    "chai": "4.2.0",
    "chai-as-promised": "^7.1.1",
